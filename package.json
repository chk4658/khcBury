{
  "name": "khc-bury",
  "version": "1.0.1",
  "description": "Extends https://www.npmjs.com/package/@xmon/bury",
  "main": "dist/index.js",
  "repository": "https://github.com/chk4658/khcBury",
  "author": "khc1szh",
  "keywords": [
    "khc1szh"
  ],
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.3.0",
    "mitt": "^3.0.0",
    "path-to-regexp": "^6.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "src/**/*.vue": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.js": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.ts": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,ts,css,md}": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "commit": "git add . && git status && git-cz",
    "prepare": "husky install",
    "build": "yarn tsc",
    "clean": "rm -rf dist pnpm* yarn.* node_modules package-*"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
